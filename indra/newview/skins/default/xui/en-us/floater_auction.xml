<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater name="auction" title="Start land sale"
 can_close="true" can_drag_on_left="false" can_minimize="true" can_resize="true"
 height="368" min_height="368" min_width="420" width="420">
	<icon bottom="88" follows="left|right|top|bottom" left="4" name="snapshot_icon"
	     right="-4" top="-24" />
	<text
	     bottom="-308" drop_shadow_visible="true" follows="left|right|bottom"
	     font="SansSerifSmall" halign="left" height="16" left="4"
	     mouse_opaque="true" name="parcel_text" width="400" />
	<check_box name="fence_check" control_name="AuctionShowFence"
	 label="Include yellow selection fence" font="SansSerifSmall"
     left="4" bottom_delta="-28" width="199" height="16" follows="left|bottom" />
	<button bottom="-360" follows="left|bottom" font="SansSerif" halign="center"
	     height="20" label="Snapshot" left="4"
	     mouse_opaque="true" name="snapshot_btn" width="96" />
	<button bottom="-360" follows="left|bottom" font="SansSerif" halign="center"
	     height="20" label="OK" left_delta="100"
	     mouse_opaque="true" name="ok_btn" width="48" />
	<string name="already for sale">
		You cannot auction parcels which are already for sale.
	</string>
</floater>
