	</menu_item_call>
	<menu_item_separator />
	<menu_item_call bottom_delta="-18" height="18" label="Save map tile" left="0"
		 mouse_opaque="true" name="save_tile" width="128">
		<on_click function="WorldMap.SaveMapTile" userdata="0" />
	</menu_item_call>
	<menu_item_call bottom_delta="-18" height="18" label="Save 3D map (planar sculpt)" left="0"
		 mouse_opaque="true" name="save_sculpt_planar" width="128">
		<on_click function="WorldMap.SaveSculpt" userdata="0" />
	</menu_item_call>
	<menu_item_separator />
	<menu_item_call bottom_delta="-18" height="18" label="Save bordered map (decal)" left="0"
		 mouse_opaque="true" name="save_decal" width="128">
		<on_click function="WorldMap.SaveMapTile" userdata="1" />
	</menu_item_call>
	<menu_item_call bottom_delta="-18" height="18" label="Save 3D map (spherical sculpt)" left="0"
		 mouse_opaque="true" name="save_sculpt_spheric" width="128">
		<on_click function="WorldMap.SaveSculpt" userdata="1" />
	</menu_item_call>
</menu>
