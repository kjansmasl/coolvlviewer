<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel name="Access" label="Access"
 left="1" bottom="1" width="498" height="480" follows="top|left">
	<tab_container name="access_tabs" label="" tab_min_width="110" tab_position="bottom"
	 left="0" bottom="-470" width="498" height="470">
		<panel name="estate_managers" label="Estate managers"
		 left="0" top="-4" width="498" height="460" follows="left|top|right|bottom">
			<text name="estate_manager_label" font="SansSerif"
			 left="10" bottom="-30" width="300" follows="left|top" height="20">
				Estate managers:
			</text>
			<button name="estate_manager_help" label="?" font="SansSerifSmall"
			 left_delta="460" bottom_delta="1" width="18" height="18" follows="left|top" />
			<name_list name="estate_manager_name_list" multi_select="true" draw_heading="false" draw_stripes="true"
			 left="10" bottom_delta="-394" width="475" height="390" follows="left|top" />
			<button name="remove_estate_manager_btn" label="Remove"
			 left="290" bottom_delta="-24" width="90" height="20" follows="left|top" />
			<button name="add_estate_manager_btn" label="Add..."
			 left_delta="100" bottom_delta="0" width="90" height="20" follows="left|top" />
		</panel>
		<panel name="allowed_groups" label="Allowed groups"
		 left="0" top="-4" width="498" height="460" follows="left|top|right|bottom">
			<text name="allow_group_label" font="SansSerif"
			 left="10" bottom="-30" width="200" follows="left|top" height="20">
				Allowed groups:
			</text>
			<button name="allow_group_help" label="?" font="SansSerifSmall"
			 left_delta="460" bottom_delta="1" width="18" height="18" follows="left|top" />
			<name_list name="allowed_group_name_list" multi_select="true" draw_heading="false" draw_stripes="true"
			 left="10" bottom_delta="-394" width="475" height="390" follows="left|top" />
			<button name="remove_allowed_group_btn" label="Remove"
			 left="290" bottom_delta="-24" width="90" height="20" follows="left|top" />
			<button name="add_allowed_group_btn" label="Add..."
			 left_delta="100" bottom_delta="0" width="90" height="20" follows="left|top" />
		</panel>
		<panel name="allowed_resident" label="Allowed residents"
		 left="0" top="-4" width="498" height="460" follows="left|top|right|bottom">
			<text name="allow_resident_label" font="SansSerif"
			 left="10" bottom="-30" width="200" follows="left|top" height="20">
				Allowed residents:
			</text>
			<button name="allow_resident_help" label="?" font="SansSerifSmall"
			 left_delta="460" bottom_delta="1" width="18" height="18" follows="left|top" />
			<name_list name="allowed_avatar_name_list" multi_select="true" draw_heading="false" draw_stripes="true"
			 left="10" bottom_delta="-394" width="475" height="390" follows="left|top" />
			<button name="remove_allowed_avatar_btn" label="Remove"
			 left="290" bottom_delta="-24" width="90" height="20" follows="left|top" />
			<button name="add_allowed_avatar_btn" label="Add..."
			 left_delta="100" bottom_delta="0" width="90" height="20" follows="left|top" />
		</panel>
		<panel name="banned_residents" label="Banned residents"
		 left="0" top="-4" width="498" height="460" follows="left|top|right|bottom">
			<text name="ban_resident_label" font="SansSerif"
			 left="10" bottom="-30" width="200" follows="left|top" height="20">
				Banned residents:
			</text>
			<button name="ban_resident_help" label="?" font="SansSerifSmall"
			 left_delta="460" bottom_delta="1" width="18" height="18" follows="left|top" />
			<name_list name="banned_avatar_name_list" multi_select="true" draw_heading="true" draw_stripes="true"
			 left="10" bottom_delta="-394" width="475" height="390" follows="left|top">
				<column name="name" label="Name" dynamicwidth="true" />
				<column name="last_login_date" label="Last login date" width="100" />
 				<column name="ban_date" label="Date banned" width="100" />
				<column name="bannedby" label="Banned by" width="140" />
			</name_list>
			<button name="remove_banned_avatar_btn" label="Remove"
			 left="290" bottom_delta="-24" width="90" height="20" follows="left|top" />
			<button name="add_banned_avatar_btn" label="Add..."
			 left_delta="100" bottom_delta="0" width="90" height="20" follows="left|top" />
		</panel>
	</tab_container>
